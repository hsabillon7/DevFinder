<main class="lista-vacantes">
    <h2>Lista de vacantes</h2>

    {{#each vacantes}}
    <div class="vacante">
        <div class="caja">
            <h3>{{empresa}}</h3>
            <p class="puesto" {{puesto}}</p> </div> <div class="caja">
                <p class="etiqueta">Ubicación</p>
                <p class="nombre">{{ubicacion}}</p>
        </div>
        <div class="caja">
            <p class="etiqueta">Contrato</p>
            <p class="nombre contrato">{{contrato}}</p>
        </div>
        <div class="caja centrar-verticalmente">
            <a href="/vacante/{{url}}" class="btn btn-verde">Más información</a>
        </div>
    </div>
    {{/each}}
</main>